<?xml version="1.0"?>
<robot name="simple_humanoid">

    <material name="torso_mat"><color rgba="0.2 0.2 0.8 1"/></material>
    <material name="limb_mat"><color rgba="0.8 0.2 0.2 1"/></material>
    <material name="foot_mat"><color rgba="0.1 0.1 0.1 1"/></material>

    <link name="torso">
        <inertial>
            <origin xyz="0 0 0.05" rpy="0 0 0"/>
            <mass value="15.0"/> 
            <inertia ixx="0.2" ixy="0" ixz="0" iyy="0.2" iyz="0" izz="0.1"/>
        </inertial>
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry><box size="0.3 0.2 0.5"/></geometry>
            <material name="torso_mat"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry><box size="0.3 0.2 0.5"/></geometry>
        </collision>
    </link>

    <joint name="right_hip_yaw" type="revolute">
        <parent link="torso"/>
        <child link="right_hip_roll_link"/>
        <origin xyz="0 -0.1 -0.25" rpy="0 0 0"/>
        <axis xyz="0 0 1"/>
        <limit lower="-0.7" upper="0.7" effort="120" velocity="10"/>
        <dynamics damping="0.5" friction="0.5"/>
    </joint>
    
    <link name="right_hip_roll_link">
        <inertial><mass value="0.01"/><inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/></inertial>
    </link>
    
    <joint name="right_hip_roll" type="revolute">
        <parent link="right_hip_roll_link"/>
        <child link="right_thigh"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <axis xyz="1 0 0"/>
        <limit lower="-1.0" upper="0.5" effort="150" velocity="10"/>
        <dynamics damping="0.5" friction="0.5"/>
    </joint>

    <link name="right_thigh">
        <inertial>
            <origin xyz="0 0 -0.2" rpy="0 0 0"/>
            <mass value="4.0"/>
            <inertia ixx="0.06" ixy="0" ixz="0" iyy="0.06" iyz="0" izz="0.015"/>
        </inertial>
        <visual>
            <origin xyz="0 0 -0.2" rpy="0 0 0"/>
            <geometry><cylinder radius="0.06" length="0.4"/></geometry>
            <material name="limb_mat"/>
        </visual>
        <collision>
            <origin xyz="0 0 -0.2" rpy="0 0 0"/>
            <geometry><cylinder radius="0.06" length="0.4"/></geometry>
        </collision>
    </link>

    <joint name="right_knee" type="revolute">
        <parent link="right_thigh"/>
        <child link="right_shin"/>
        <origin xyz="0 0 -0.4" rpy="0 0 0"/>
        <axis xyz="0 1 0"/>
        <limit lower="0" upper="2.5" effort="150" velocity="10"/>
        <dynamics damping="0.5" friction="0.5"/>
    </joint>
    
    <link name="right_shin">
        <inertial>
            <origin xyz="0 0 -0.2" rpy="0 0 0"/>
            <mass value="3.0"/>
            <inertia ixx="0.04" ixy="0" ixz="0" iyy="0.04" iyz="0" izz="0.01"/>
        </inertial>
        <visual>
            <origin xyz="0 0 -0.2" rpy="0 0 0"/>
            <geometry><cylinder radius="0.05" length="0.4"/></geometry>
            <material name="limb_mat"/>
        </visual>
        <collision>
            <origin xyz="0 0 -0.2" rpy="0 0 0"/>
            <geometry><cylinder radius="0.05" length="0.4"/></geometry>
        </collision>
    </link>

    <joint name="right_ankle_roll" type="revolute">
        <parent link="right_shin"/>
        <child link="right_foot"/>
        <origin xyz="0 0 -0.4" rpy="0 0 0"/>
        <axis xyz="1 0 0"/>
        <limit lower="-0.3" upper="0.3" effort="80" velocity="10"/>
        <dynamics damping="0.2" friction="0.2"/>
    </joint>
    
    <link name="right_foot">
        <inertial>
            <origin xyz="0.05 0 -0.015" rpy="0 0 0"/>
            <mass value="1.0"/>
            <inertia ixx="0.002" ixy="0" ixz="0" iyy="0.005" iyz="0" izz="0.003"/>
        </inertial>
        <visual>
            <origin xyz="0.05 0 -0.015" rpy="0 0 0"/>
            <geometry><box size="0.15 0.08 0.03"/></geometry>
            <material name="foot_mat"/>
        </visual>
        <collision>
            <origin xyz="0.05 0 -0.015" rpy="0 0 0"/>
            <geometry><box size="0.15 0.08 0.03"/></geometry>
        </collision>
    </link>

    <joint name="left_hip_yaw" type="revolute">
        <parent link="torso"/>
        <child link="left_hip_roll_link"/>
        <origin xyz="0 0.1 -0.25" rpy="0 0 0"/>
        <axis xyz="0 0 1"/>
        <limit lower="-0.7" upper="0.7" effort="120" velocity="10"/>
        <dynamics damping="0.5" friction="0.5"/>
    </joint>
    
    <link name="left_hip_roll_link">
        <inertial><mass value="0.01"/><inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/></inertial>
    </link>
    
    <joint name="left_hip_roll" type="revolute">
        <parent link="left_hip_roll_link"/>
        <child link="left_thigh"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <axis xyz="1 0 0"/>
        <limit lower="-0.5" upper="1.0" effort="150" velocity="10"/>
        <dynamics damping="0.5" friction="0.5"/>
    </joint>

    <link name="left_thigh">
        <inertial><origin xyz="0 0 -0.2" rpy="0 0 0"/><mass value="4.0"/><inertia ixx="0.06" ixy="0" ixz="0" iyy="0.06" iyz="0" izz="0.015"/></inertial>
        <visual><origin xyz="0 0 -0.2" rpy="0 0 0"/><geometry><cylinder radius="0.06" length="0.4"/></geometry><material name="limb_mat"/></visual>
        <collision><origin xyz="0 0 -0.2" rpy="0 0 0"/><geometry><cylinder radius="0.06" length="0.4"/></geometry></collision>
    </link>

    <joint name="left_knee" type="revolute">
        <parent link="left_thigh"/>
        <child link="left_shin"/>
        <origin xyz="0 0 -0.4" rpy="0 0 0"/>
        <axis xyz="0 1 0"/>
        <limit lower="0" upper="2.5" effort="150" velocity="10"/>
        <dynamics damping="0.5" friction="0.5"/>
    </joint>
    
    <link name="left_shin">
        <inertial><origin xyz="0 0 -0.2" rpy="0 0 0"/><mass value="3.0"/><inertia ixx="0.04" ixy="0" ixz="0" iyy="0.04" iyz="0" izz="0.01"/></inertial>
        <visual><origin xyz="0 0 -0.2" rpy="0 0 0"/><geometry><cylinder radius="0.05" length="0.4"/></geometry><material name="limb_mat"/></visual>
        <collision><origin xyz="0 0 -0.2" rpy="0 0 0"/><geometry><cylinder radius="0.05" length="0.4"/></geometry></collision>
    </link>

    <joint name="left_ankle_roll" type="revolute">
        <parent link="left_shin"/>
        <child link="left_foot"/>
        <origin xyz="0 0 -0.4" rpy="0 0 0"/>
        <axis xyz="1 0 0"/>
        <limit lower="-0.3" upper="0.3" effort="80" velocity="10"/>
        <dynamics damping="0.2" friction="0.2"/>
    </joint>
    
    <link name="left_foot">
        <inertial><origin xyz="0.05 0 -0.015" rpy="0 0 0"/><mass value="1.0"/><inertia ixx="0.002" ixy="0" ixz="0" iyy="0.005" iyz="0" izz="0.003"/></inertial>
        <visual><origin xyz="0.05 0 -0.015" rpy="0 0 0"/><geometry><box size="0.15 0.08 0.03"/></geometry><material name="foot_mat"/></visual>
        <collision><origin xyz="0.05 0 -0.015" rpy="0 0 0"/><geometry><box size="0.15 0.08 0.03"/></geometry></collision>
    </link>
    
    <link name="right_shoulder_roll_link"><inertial><mass value="0.01"/><inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/></inertial></link>
    <joint name="right_shoulder_roll" type="revolute">
        <parent link="torso"/>
        <child link="right_shoulder_roll_link"/>
        <origin xyz="0 -0.15 0.15" rpy="0 0 0"/>
        <axis xyz="1 0 0"/> <limit lower="-1.0" upper="0.5" effort="60" velocity="10"/>
        <dynamics damping="0.1" friction="0.1"/>
    </joint>
    <joint name="right_shoulder_pitch" type="revolute">
        <parent link="right_shoulder_roll_link"/>
        <child link="right_upper_arm"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <axis xyz="0 1 0"/> <limit lower="-3.14" upper="3.14" effort="60" velocity="10"/>
        <dynamics damping="0.1" friction="0.1"/>
    </joint>
    <link name="right_upper_arm">
        <inertial><origin xyz="0 0 -0.15" rpy="0 0 0"/><mass value="1.5"/><inertia ixx="0.02" ixy="0" ixz="0" iyy="0.02" iyz="0" izz="0.005"/></inertial>
        <visual><origin xyz="0 0 -0.15" rpy="0 0 0"/><geometry><cylinder radius="0.04" length="0.3"/></geometry><material name="limb_mat"/></visual>
        <collision><origin xyz="0 0 -0.15" rpy="0 0 0"/><geometry><cylinder radius="0.04" length="0.3"/></geometry></collision>
    </link>
    
    <link name="left_shoulder_roll_link"><inertial><mass value="0.01"/><inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/></inertial></link>
    <joint name="left_shoulder_roll" type="revolute">
        <parent link="torso"/>
        <child link="left_shoulder_roll_link"/>
        <origin xyz="0 0.15 0.15" rpy="0 0 0"/>
        <axis xyz="1 0 0"/>
        <limit lower="-0.5" upper="1.0" effort="60" velocity="10"/>
        <dynamics damping="0.1" friction="0.1"/>
    </joint>
    <joint name="left_shoulder_pitch" type="revolute">
        <parent link="left_shoulder_roll_link"/>
        <child link="left_upper_arm"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <axis xyz="0 1 0"/>
        <limit lower="-3.14" upper="3.14" effort="60" velocity="10"/>
        <dynamics damping="0.1" friction="0.1"/>
    </joint>
    <link name="left_upper_arm">
        <inertial><origin xyz="0 0 -0.15" rpy="0 0 0"/><mass value="1.5"/><inertia ixx="0.02" ixy="0" ixz="0" iyy="0.02" iyz="0" izz="0.005"/></inertial>
        <visual><origin xyz="0 0 -0.15" rpy="0 0 0"/><geometry><cylinder radius="0.04" length="0.3"/></geometry><material name="limb_mat"/></visual>
        <collision><origin xyz="0 0 -0.15" rpy="0 0 0"/><geometry><cylinder radius="0.04" length="0.3"/></geometry></collision>
    </link>

</robot>